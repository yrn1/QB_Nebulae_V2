nebconfigbegin
sr,48000
ksmps,64
-B,512
-b,64
nebconfigend


instr 1

ainl, ainr inch 1, 2
if gksource == 1 then
  ires system_i 1,{{
          if echo alarm | sudo -S mount /dev/sda1 /mnt/memory; then echo copy wificonfig; sudo cp /mnt/memory/wlan0-wifi /etc/netctl; sudo umount /dev/sda1; fi
          echo alarm | sudo -S netctl start wlan0-wifi
          echo wifi done
      }}
  print ires
  turnoff
endif
outs ainl, ainr

endin